{
    "name": "adrhumphreys/silverstripe-fixtures",
    "description": "Fixtures for Silverstripe.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Fixtures",
        "Test"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.1",
        "silverstripe/framework": "^4.0 || ^5.0",
        "silverstripe/admin": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AdrHumphreys\\Fixtures\\": "src/",
            "AdrHumphreys\\Fixtures\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
